{
  "name": "phosphorus",
  "version": "1.0.0",
  "description": "rotur web engine",
  "main": "script.osl",
  "dependencies": {},
  "tags": [],
  "include": [
    "./main.rwl",
    "./test.rwl",
    "./sidebartest.rwl",
    "./src/assets/empty.rwl",
    "./src/assets/settings.rwl"
  ],
  "phosphorus": {
    "layout": "summit"
  },
  "scripts": {
    "opaljson-summit": "osl ./scripts/opal_summit.osl",
    "build-summit": [
      "fpp script opaljson-summit",
      "fpp build",
      "rm build/script-summit.osl",
      "mv build/script.osl build/script-summit.osl",
      "echo \"summit build complete!\""
    ],
    "opaljson-chromeLike": "osl ./scripts/opal_chromeLike.osl",
    "build-chromeLike": [
      "fpp script opaljson-chromeLike",
      "fpp build",
      "rm build/script-chromeLike.osl",
      "mv build/script.osl build/script-chromeLike.osl",
      "echo \"chromeLike build complete!\""
    ],
    "build": [
      "fpp script build-summit",
      "fpp script build-chromeLike"
    ]
  }
}